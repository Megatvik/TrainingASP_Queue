@model IEnumerable<Queue.Models.QueryView>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table" style="">
    
    <tr>
        <th colspan="4">Доступные запросы:</th>
    </tr>
    <tr>
        <th>Название</th>
        <th>Подписано экспертов</th>
        <th>Размер очереди</th>
        <th>Вне очереди</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.NameQuery</td>
            <td>@item.ExpertsCount</td>
            <td>@item.QueueCount</td>
            <td>@item.SubQueueCount</td>
            <td>
                @Html.ActionLink("Изменить", "Edit", "Admin", new { QID = item.ID }, null)
                @Html.ActionLink("Удалить", "Delete", "Admin", new { QID = item.ID }, null)
            </td>
        </tr>
    }
</table>

